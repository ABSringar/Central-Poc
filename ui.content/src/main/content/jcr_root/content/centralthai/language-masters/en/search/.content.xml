<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" 
    xmlns:cq="http://www.day.com/jcr/cq/1.0" 
    xmlns:jcr="http://www.jcp.org/jcr/1.0" 
    jcr:primaryType="cq:Page">
    <jcr:content cq:template="/conf/centralthai/settings/wcm/templates/page-content" 
        jcr:primaryType="cq:PageContent" 
        jcr:title="centralthai Demo Store - Search Results"
        sling:resourceType="centralthai/components/page" 
        hideInNav="true">
        <root jcr:primaryType="nt:unstructured" 
            sling:resourceType="centralthai/components/container" 
            layout="responsiveGrid">
            <container jcr:primaryType="nt:unstructured" 
                sling:resourceType="centralthai/components/container">
                <container jcr:primaryType="nt:unstructured" 
                    sling:resourceType="centralthai/components/container" 
                    layout="responsiveGrid">
                    <searchresults jcr:primaryType="nt:unstructured" 
                        sling:resourceType="centralthai/components/commerce/searchresults" />
                </container>
            </container>
        </root>
    </jcr:content>
</jcr:root>
